-- catchtable.tbl_user definition

CREATE TABLE `tbl_user` (
  `mno` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(10) NOT NULL,
  `phone` varchar(12) NOT NULL,
  `password` varchar(100) NOT NULL,
  `nickname` varchar(20) NOT NULL,
  `date` date NOT NULL,
  PRIMARY KEY (`mno`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- catchtable.tbl_terms definition

CREATE TABLE `tbl_terms` (
  `tno` int(11) NOT NULL AUTO_INCREMENT,
  `mno` int(11) NOT NULL,
  `peristalsis` varchar(1) NOT NULL,
  `terms_1` varchar(1) NOT NULL,
  `terms_2` varchar(1) NOT NULL,
  `terms_3` varchar(1) NOT NULL,
  `terms_4` varchar(1) NOT NULL,
  `terms_5` varchar(1) NOT NULL,
  `terms_6_1` varchar(1) NOT NULL,
  `terms_6_2` varchar(1) NOT NULL,
  `terms_6_3` varchar(1) NOT NULL,
  PRIMARY KEY (`tno`),
  KEY `tbl_terms_FK` (`mno`),
  CONSTRAINT `tbl_terms_FK` FOREIGN KEY (`mno`) REFERENCES `tbl_user` (`mno`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;